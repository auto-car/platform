{
  "name": "autocar-platform",
  "description": "The grand mono-repo for all AutoCAR platform related services.",
  "author": "Adi Kishore <kishek12@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:autocar-platform-workspace:^.git"
  },
  "scripts": {
    "build": "tsc --build",
    "build:force": "tsc --build --force",
    "build:clean": "rm -rf packages/**/dist/**",
    "frontend:dev": "yarn workspace @platform/frontend dev",
    "backend:dev": "cd packages/backend && ./run-backend.sh",
    "user-worker:dev": "yarn workspace @platform/user-worker dev",
    "user-worker:publish": "yarn workspace @platform/user-worker publish"
  },
  "packageManager": "yarn@3.3.1",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/workers/*"
  ],
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230115.0",
    "@platform/frontend": "workspace:^",
    "@platform/model": "workspace:^",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-typescript": "^3.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.4.0",
    "miniflare": "^2.11.0",
    "rollup": "^3.12.0",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "^4.9.4",
    "wrangler": "^2.8.1"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^2.2.1",
    "@liveblocks/client": "^0.19.7",
    "@liveblocks/node": "^0.19.7",
    "@liveblocks/react": "^0.19.7",
    "@next/font": "13.0.7",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "eslint": "8.30.0",
    "eslint-config-next": "13.0.7",
    "next": "13.0.7",
    "normalize.css": "^8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-spinners": "^0.13.8",
    "typescript": "4.9.4"
  }
}
