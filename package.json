{
  "name": "autocar-platform",
  "description": "The grand mono-repo for all AutoCAR platform related services.",
  "author": "Adi Kishore <kishek12@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:autocar-platform-workspace:^.git"
  },
  "scripts": {
    "build": "tsc --build",
    "build:force": "tsc --build --force",
    "build:clean": "rm -rf packages/**/dist/**",
    "frontend:dev-remix": "yarn workspace @platform/remix-frontend dev:remix",
    "frontend:dev": "yarn workspace @platform/remix-frontend dev",
    "frontend:build": "yarn build && yarn workspace @platform/remix-frontend build",
    "backend:dev": "cd packages/backend && ./run-backend.sh",
    "user-worker:dev": "yarn workspace @platform/user-worker dev",
    "user-worker:publish": "yarn workspace @platform/user-worker publish",
    "team-worker:dev": "yarn workspace @platform/team-worker dev",
    "team-worker:publish": "yarn workspace @platform/team-worker publish",
    "lab-worker:dev": "yarn workspace @platform/lab-worker dev",
    "lab-worker:publish": "yarn workspace @platform/lab-worker publish",
    "analysis-worker:dev": "yarn workspace @platform/analysis-worker dev",
    "analysis-worker:publish": "yarn workspace @platform/analysis-worker publish",
    "workers:publish": "./publish-workers.sh",
    "workspaces:list": "yarn workspaces list --json"
  },
  "packageManager": "yarn@3.3.1",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/workers/*"
  ],
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230115.0",
    "@platform/analysis-worker": "workspace:^",
    "@platform/frontend": "workspace:^",
    "@platform/lab-worker": "workspace:^",
    "@platform/model": "workspace:^",
    "@platform/remix-frontend": "workspace:^",
    "@platform/room-worker": "workspace:^",
    "@platform/team-worker": "workspace:^",
    "@platform/user-worker": "workspace:^",
    "@remix-run/cloudflare": "^1.14.0",
    "@remix-run/cloudflare-pages": "^1.14.0",
    "@remix-run/cloudflare-workers": "^1.14.0",
    "@remix-run/css-bundle": "^1.14.0",
    "@remix-run/node": "^1.14.0",
    "@remix-run/react": "^1.14.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-typescript": "^3.0.0",
    "isbot": "^3.6.5",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.4.0",
    "miniflare": "^2.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.4.0",
    "remix-auth-auth0": "^1.6.0",
    "rollup": "^3.12.0",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "^4.9.4",
    "wrangler": "^2.8.1"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^2.2.1",
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.4.9",
    "@chakra-ui/system": "^2.3.8",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@liveblocks/client": "^0.19.7",
    "@liveblocks/node": "^0.19.7",
    "@liveblocks/react": "^0.19.7",
    "@next/font": "13.0.7",
    "@react-hook/size": "^2.1.2",
    "@remix-run/dev": "^1.14.0",
    "@remix-run/eslint-config": "^1.14.0",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "eslint": "8.30.0",
    "eslint-config-next": "13.0.7",
    "framer-motion": "^9.0.1",
    "next": "13.0.7",
    "nodemon": "^2.0.20",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-select": "^5.7.0",
    "react-spinners": "^0.13.8",
    "react-zoom-pan-pinch": "^2.5.0",
    "typescript": "^4.8.4",
    "wrangler": "^2.2.1"
  }
}
